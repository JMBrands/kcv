switch (owner.class) {
	case classes.dimachaerus:
		sprite_index = false;
		break;
	case classes.hoplomachus:
		sprite_index = sHoplomachusArm;
		break;
	case classes.murmillo:
		sprite_index = sMurmilloArm;
		break;
	case classes.provocator:
		sprite_index = sProvocatorComplete;
		break;
	case classes.retiarius:
		sprite_index = sRetiariusArm;
		break;
	case classes.thraex:
		sprite_index = false;
		break;
	default:
		
		break;
}
